-cp src
-lib gmodhaxe
-lib safety
-lib tink_json
-lib tink_core
-lib profiler
--times
-D macro-times
-D gmddebug
-D addonName=debugee
-D analyzer-optimize
-D dump=pretty
-D noGenInit
-D haxeJSON
-D lua-ver=5.1
--resource src/gmdebug/lua/include/XPCall.lua@XPCall
--resource src/gmdebug/lua/include/Catch.lua@Catch
-dce full
--macro gmod.helpers.macros.InitMacro.init()
--macro gmdebug.lua.macros.InitMacro.addDebugContext()
--cmd lix -lib gmodhaxe --run gmod.helpers.macros.PostCompileMacro